<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.Cursor?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="550.0" prefWidth="800.0" xmlns="http://javafx.com/javafx/8.0.141" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.RegistrationIssueController">
   <children>
      <Label fx:id="lblHeading" alignment="CENTER" layoutX="155.0" layoutY="71.0" prefHeight="16.0" prefWidth="500.0" textFill="#d413aa">
         <font>
            <Font name="Comic Sans MS Bold" size="28.0" />
         </font>
      </Label>
      <Label alignment="CENTER_RIGHT" layoutX="183.0" layoutY="158.0" prefHeight="30.0" prefWidth="150.0" text="Username : " />
      <Label alignment="CENTER_RIGHT" layoutX="183.0" layoutY="203.0" prefHeight="30.0" prefWidth="150.0" text="Password : " />
      <Label fx:id="lblReTypePassword" alignment="CENTER_RIGHT" layoutX="183.0" layoutY="248.0" prefHeight="30.0" prefWidth="150.0" text="Re-Type Password : " />
      <Label alignment="CENTER_RIGHT" layoutX="163.0" layoutY="293.0" prefHeight="30.0" prefWidth="170.0" text="Security Question(S.Q.) : " />
      <Label alignment="CENTER_RIGHT" layoutX="183.0" layoutY="338.0" prefHeight="30.0" prefWidth="150.0" text="Answer : " />
      <TextField fx:id="txtUsername" layoutX="333.0" layoutY="158.0" onKeyReleased="#txtUsername" prefHeight="30.0" prefWidth="200.0" promptText="Username" />
      <PasswordField fx:id="passPassword" layoutX="333.0" layoutY="203.0" onKeyReleased="#passPassword" prefHeight="30.0" prefWidth="200.0" promptText="Password" />
      <PasswordField fx:id="passReTypePassword" layoutX="333.0" layoutY="248.0" onKeyReleased="#passReTypePassword" prefHeight="30.0" prefWidth="200.0" promptText="Re-Type Password" />
      <ComboBox fx:id="cmboSecurityQuestion" layoutX="333.0" layoutY="293.0" prefHeight="30.0" prefWidth="315.0">
         <cursor>
            <Cursor fx:constant="CLOSED_HAND" />
         </cursor></ComboBox>
      <TextField fx:id="txtAnswer" layoutX="333.0" layoutY="338.0" onKeyReleased="#txtAnswer" prefHeight="30.0" prefWidth="200.0" promptText="Answer the Question" />
      <Button fx:id="btnDelete" focusTraversable="false" layoutX="428.0" layoutY="398.0" mnemonicParsing="false" onAction="#btnDelete" prefHeight="30.0" prefWidth="100.0" text="Delete">
         <cursor>
            <Cursor fx:constant="CLOSED_HAND" />
         </cursor></Button>
      <Button fx:id="btnCancel" layoutX="272.0" layoutY="398.0" mnemonicParsing="false" onAction="#btnCancel" prefHeight="30.0" prefWidth="100.0" text="Cancel">
         <cursor>
            <Cursor fx:constant="CLOSED_HAND" />
         </cursor></Button>
      <Label fx:id="lblMsgPassword" layoutX="539.0" layoutY="253.0" prefHeight="20.0" prefWidth="150.0" textFill="RED">
         <font>
            <Font size="12.0" />
         </font>
      </Label>
      <Label fx:id="lblMsgInformation" alignment="CENTER" layoutX="278.0" layoutY="123.0" prefHeight="20.0" prefWidth="250.0" textFill="RED">
         <font>
            <Font size="12.0" />
         </font>
      </Label>
      <Button fx:id="btnSave" focusTraversable="false" layoutX="428.0" layoutY="398.0" mnemonicParsing="false" onAction="#btnSave" prefHeight="30.0" prefWidth="100.0" text="Save">
         <cursor>
            <Cursor fx:constant="CLOSED_HAND" />
         </cursor></Button>
   </children>
</AnchorPane>
