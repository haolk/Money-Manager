            <Font name="Arial" size="16.0" />
         </font>
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor>
      </Label>
      <Label fx:id="lblNewUser" alignment="CENTER" focusTraversable="false" layoutX="498.0" layoutY="417.0" onMouseClicked="#newUser" prefHeight="30.0" prefWidth="200.0" text="New User">
         <font>
            <Font name="Arial" size="16.0" />
         </font>
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor>
      </Label>
      <TextField fx:id="txtSQAnswer" focusTraversable="false" layoutX="80.0" layoutY="256.0" onMouseClicked="#forgotPasswordtxtFieldFocus" prefHeight="30.0" prefWidth="270.0" promptText="Answer Here" />
      <Label fx:id="lblForgetPassMsg" alignment="CENTER" contentDisplay="CENTER" focusTraversable="false" layoutX="80.0" layoutY="193.0" prefHeight="20.0" prefWidth="270.0" textAlignment="CENTER" textFill="RED" textOverrun="CLIP">
