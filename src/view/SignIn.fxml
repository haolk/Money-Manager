            <Font size="13.0" />
         </font>
      </Label>
      <ImageView fx:id="imgSignIn" fitHeight="331.0" fitWidth="355.0" layoutX="63.0" layoutY="150.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../imges/signin3.jpg" />
         </image>
      </ImageView>
   </children>
</AnchorPane>
